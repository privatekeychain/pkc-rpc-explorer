extends layout

block headContent
	if(session.locate=="english")
		title Block ##{result.getblock.height.toLocaleString()}, #{result.getblock.hash}
	else
		title 区块 ##{result.getblock.height.toLocaleString()}, #{result.getblock.hash}

block content
	span(class="h3" i18n-content="Block")  
	span(style="width: 100%;font-size:20px")      ##{result.getblock.height.toLocaleString()}
	br
	span(style="width: 100%;font-size:20px") #{result.getblock.hash}
	hr

	include includes/block-content.pug